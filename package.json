{
  "name": "reactotool",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "author": {
    "name": "Younanator",
    "email": "N/A",
    "url": "N/A"
  },
  "build": {
    "appId": "ReactoTool",
    "productName": "ReactoTool",
    "copyright": "Copyright Â© 2019 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": false
    },
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.19.2",
    "cors": "^2.8.5",
    "electron-is-dev": "^1.1.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^0.20.0",
    "ip": "^1.1.5",
    "node-cmd": "^3.0.0",
    "node-powershell": "^4.0.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.6",
    "powershell": "^2.3.2",
    "react": "^16.12.0",
    "react-barcode-reader": "^0.0.2",
    "react-beautiful-dnd": "^12.2.0",
    "react-d3-tree": "^1.16.1",
    "react-dom": "^16.12.0",
    "react-loading-skeleton": "^2.0.1",
    "react-qr-scanner": "^0.0.9",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "react-toastify": "^5.5.0",
    "react-uuid": "^1.0.2",
    "react-webcam-barcode-scanner": "^0.0.2-rc1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test",
    "edev": "concurrently \"BROWSER=none yarn start\"  \"wait-on http://localhost:3000 && electron .\" \"react-scripts start\" ",
    "serv": "nodemon ./server/server.js",
    "eject": "react-scripts eject",
    "postinstall": "electron-builder install-app-deps",
    "prepack": "yarn build",
    "epack": "electron-builder build -w"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rescripts/cli": "^0.0.13",
    "@rescripts/rescript-env": "^0.0.11",
    "@types/office-js": "^1.0.66",
    "concurrently": "^5.1.0",
    "electron": "^7.1.10",
    "electron-builder": "^22.3.2",
    "typescript": "^3.7.5",
    "wait-on": "^4.0.0"
  }
}
